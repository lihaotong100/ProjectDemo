<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="crm.domain.SysUserGroup" table="sys_user_group">
		<id name="id" type="integer">
			<column name="id" />
			<generator class="identity"></generator>
		</id>
		
		<property name="name" type="string">
			<column name="name" sql-type="varchar(100)"></column>
		</property>
		
		<property name="principal" type="string">
			<column name="principal" sql-type="varchar(50)"></column>
		</property>

		<property name="incumbent" type="string">
			<column name="incumbent" sql-type="varchar(200)"></column>
		</property>				
		
		<property name="remark" type="text">
			<column name="remark" ></column>
		</property>		
    </class>
</hibernate-mapping>
